{
  "name": "store-manager-app",
  "version": "1.0.0",
  "author": "rramadev",
  "description": "Angular2 Demo - Store Manager App",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "start": "npm run build:tsc && concurrently \"npm run build:tsc:w\" \"npm run server\" ",
    "server": "lite-server -c bs-config.json",
    "server:prod": "lite-server -c bs-config-prod.json",
    "server:npm": "node server.js",
    "build:tsc": "tsc -p src/",
    "build:tsc:w": "tsc -p src/ -w",
    "build:gulp": "gulp build",
    "build:sass": "node-sass --output-style compressed src/styles.scss src/styles.min.css ",
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "rollup": "rollup",
    "build:rollup": "rollup -c rollup.config.js",
    "build:rollup:vendor": "rollup -c rollup.config.vendor.js",
    "build:rollup:tsc": "tsc --out ./dist/js/main.bundle.js --target es5 --allowJs dist/js/main.es2015.js",
    "build:rollup:vendor:tsc": "tsc --out ./dist/js/vendor.bundle.js --target es5 --allowJs dist/js/vendor.es2015.js",
    "minify:main": "uglifyjs dist/js/main.bundle.js --screw-ie8 --compress --mangle --output dist/js/main.bundle.min.js",
    "minify:vendor": "uglifyjs dist/js/vendor.bundle.js --screw-ie8 --compress --mangle --output dist/js/vendor.bundle.min.js"
  },
  "dependencies": {
    "@angular/common": "^2.4.10",
    "@angular/compiler": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/forms": "^2.4.10",
    "@angular/http": "^2.4.10",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^2.4.10",
    "@angular/platform-browser-dynamic": "^2.4.10",
    "@angular/router": "^3.4.10",
    "angular-in-memory-web-api": "^0.2.5",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "ng2-ui": "^0.13.4",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "zone.js": "^0.7.8"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.37",
    "@types/google-maps": "^3.2.0",
    "@types/hammerjs": "^2.0.34",
    "@types/node": "^6.0.65",
    "body-parser": "^1.17.1",
    "codelyzer": "^2.0.1",
    "colors": "^1.1.2",
    "concurrently": "^3.4.0",
    "del": "^2.2.2",
    "express": "^4.15.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.0.4",
    "gulp-concat": "^2.6.1",
    "gulp-concat-js": "^0.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-tslint": "^7.1.0",
    "gulp-typescript": "^3.1.6",
    "gulp-uglify": "^2.1.2",
    "lite-server": "^2.3.0",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.2.1",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-commonjs": "^7.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "run-sequence": "^1.2.2",
    "systemjs": "^0.19.46",
    "systemjs-builder": "^0.15.36",
    "tsconfig-glob": "^0.4.3",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "uglifyjs": "^2.4.10"
  },
  "repository": {}
}
